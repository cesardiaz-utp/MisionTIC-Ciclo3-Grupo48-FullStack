ategories\" :key=\"category\">\n                {{ category }}\n              </v-chip>\n            </v-chip-group>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"success\">Editar</v-btn>\n              <v-btn color=\"error\">Eliminar</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { getAllProducts } from \"../services/Products.service\";\n\nexport default {\n  data() {\n    return {\n      products: [],\n    };\n  },\n  mounted() {\n    getAllProducts()\n      .then((response) => {\n        this.products = response.data;\n      })\n      .catch((err) => console.error(err));\n  },\n};\n</script>\n\n<style scope>\n\n\n.v-card {\n  min-width: 250px;\n}\n\n.v-slide-group__content {\n  justify-content: center;\n}\n</style>"]}]}