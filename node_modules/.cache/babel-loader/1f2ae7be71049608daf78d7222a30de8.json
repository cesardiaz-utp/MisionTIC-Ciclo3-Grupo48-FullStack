"text-h6\"> {{ title }} </v-list-item-title>\n        <v-list-item-subtitle> {{ username }} </v-list-item-subtitle>\n      </v-list-item-content>\n    </v-list-item>\n\n    <v-divider></v-divider>\n\n    <v-list dense nav>\n      <div v-for=\"item in items\" :key=\"item.nombre\">\n        <v-list-item link :to=\"item.ruta\" v-if=\"item.ver\">\n          <v-list-item-icon>\n            <v-icon>{{ item.imagen }}</v-icon>\n          </v-list-item-icon>\n\n          <v-list-item-content>\n            <v-list-item-title>{{ item.nombre }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </div>\n    </v-list>\n  </v-navigation-drawer>\n</template>\n\n<script>\nexport default {\n  props: [\"title\", \"drawer\"],\n  data() {\n    return {\n      username: \"\",\n      items: [\n        { nombre: \"Inicio\", imagen: \"mdi-home\", ruta: \"/\", ver: true },\n        {\n          nombre: \"Productos\",\n          imagen: \"mdi-apps-box\",\n          ruta: \"/products\",\n          ver: this.hasRole(\"admin\"),\n        },\n        {\n          nombre: \"Acerca de\",\n          imagen: \"mdi-help-box\",\n          ruta: \"/about\",\n          ver: true,\n        },\n      ],\n    };\n  },\n  mounted(){\n    this.username = sessionStorage.getItem(\"username\");\n  },\n  methods: {\n    hasRole(role) {\n      return sessionStorage.getItem(\"role\") == role;\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}